module testsuites/constraint-generation/rules

imports

  signatures/testsuites/constraint-generation/-

rules

  [[ WildcardParam(t) ^ () ]] :=
    WildcardParam[[ t ^ (42) ]].
  WildcardParam[[ t ^ (_) ]] := false | note.


  [[ WildcardType(t) ^ () ]] :=
    WildcardType[[ t ^ () : 42 ]].
  WildcardType[[ t ^ () : _ ]] := false | note.


  [[ WildcardConstraint() ^ () ]] :=
    _ == 42, false | note.

rules

  [[ CallRuleHOOnList(ts) ^ () ]] :=
    Map0(CallRuleHOOnList)[[ ts ^ () ]].
  CallRuleHOOnList[[ t ^ () ]] := false | note.

rules

  [[ NewScopeAsType(x) ^ () ]] :=
    NewScopeAsType[[ x ^ () : _ ]].
  NewScopeAsType[[ x ^ () : s ]] :=
    new s.

  [[ NewOnParameter(t) ^ () ]] :=
    NewOnParameter[[ t ^ (s) ]].
  NewOnParameter[[ t ^ (s) ]] :=
    new s.

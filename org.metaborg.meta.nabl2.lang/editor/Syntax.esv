module Syntax

imports

  libspoofax/color/default
  completion/colorer/NaBL2Lang-cc-esv

language

  table         : target/metaborg/sdf.tbl
  start symbols : Start

  line comment  : "//"
  block comment : "/*" * "*/"
  fences        : [[ ]] ( ) { } [ ]

views

  outline view  : editor-outline
    expand to level: 3
